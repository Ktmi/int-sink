phony += all

all: $(target-exec)

$(target-exec): $(obj-user) $(OBJECT_LIBBPF)
	$(LINK.o) $^ $(LOADLIBES) $(LDLIBS) -o $@

$(obj-user): %.o: %.c $(OBJECT_LIBBPF)
	$(COMPILE.c) $(OUTPUT_OPTION) $<

phony += clean

clean:
	-rm -f $(target-exec)
	-rm -f $(obj-user)

.PHONY: $(phony)

.DELETE_ON_ERROR:

.SECONDARY: